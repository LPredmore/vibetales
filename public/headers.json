{
  "manifest.json": {
    "Cache-Control": "no-cache, no-store, must-revalidate, max-age=0",
    "Pragma": "no-cache",
    "Expires": "0",
    "ETag": "W/\"manifest-version-change\""
  },
  "app-version.json": {
    "Cache-Control": "no-cache, no-store, must-revalidate, max-age=0",
    "Pragma": "no-cache",
    "Expires": "0"
  },
  "build-version.txt": {
    "Cache-Control": "no-cache, no-store, must-revalidate, max-age=0",
    "Pragma": "no-cache",
    "Expires": "0"
  },
  "**/*.js": {
    "Cache-Control": "no-cache, no-store, must-revalidate"
  },
  "**/*.css": {
    "Cache-Control": "no-cache, no-store, must-revalidate"
  }
}